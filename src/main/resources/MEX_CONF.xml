<MyMEX>
	<MEXCORE>
		<BASIC_ENTITIES>
			<EXPERIMENT>
				<ID>EX001</ID>
				<DESC>MY PIPELINE DEMO EXPERIMENT</DESC>
				<DATE></DATE>
				<AUTHOR>
					<NAME>Esteves</NAME>
					<EMAIL>esteves@informatik.uni-leipzig.de</EMAIL>
					<HOMEPAGE></HOMEPAGE>
					<PUBLICATION_TRUSTYURI></PUBLICATION_TRUSTYURI>
				</AUTHOR>
				<PREPROCESSING_INFO>
					<DATA_NORMALIZED></DATA_NORMALIZED>
					<OUTLIERS_REMOVED></OUTLIERS_REMOVED>
					<NOISE_REMOVED></NOISE_REMOVED>
					<ATTRIBUTE_SELECTION></ATTRIBUTE_SELECTION>
				</PREPROCESSING_INFO>
				<CONTEXT MEXID="FINANCE"></CONTEXT>
			</EXPERIMENT>
			<IMPLEMENTATION ID="WEKA" VERSION="3.6.6"/>
			<SAMPLING_METHOD SID="SM1" ID="CROSS_VALIDATION">
				<FOLDS>10</FOLDS>
				<RANDOM>FALSE</RANDOM>
				<TRAIN_SIZE>.8</TRAIN_SIZE>
				<TEST_SIZE>.2</TEST_SIZE>
				<NR_ITERACTIONS></NR_ITERACTIONS>
			</SAMPLING_METHOD>
			<DATASET SID="DS1">
				<NAME>DS_SAMPLE</NAME>
				<DESCRIPTON>...</DESCRIPTION>
				<PATH>HTTP://DS/001.TXT</PATH>
			</DATASET>
			<HARDWARE SID="HC1">
				<OS>MAC OS</OS>
				<CPU>CORE I7</CPU>
				<MEMORY>8GB</MEMORY>
				<HD_TYPE>SSD</HD_TYPE>
				<CPU_CACHE>3MB</CPU_CACHE>
				<VIDEO_GRAPH></VIDEO_GRAPH>
			</HARDWARE>
			<FEATURES>
					<F SID="F1" ID="open" LABEL="open price"></F>
					<F SID="F2" ID="close" LABEL="close price"></F>
					<F SID="F3" ID="min" LABEL="minimum price"></F>
					<F SID="F4" ID="max" LABEL="maximum price"></F>
			</FEATURES>
		</BASIC_ENTITIES>
		<CONFIGURATIONS>
			<CONFIGURATION SID="CONF1" DESC="">
				<SAMPLING_METHOD>SM1</SAMPLING_METHOD>
				<DATASET>DS1</DATASET>
				<HARDWARE>HC1</HARDWARE>
				<ALGORITHMS>
					<ALGORITHM>A1</ALGORITHM>
					<ALGORITHM>A2</ALGORITHM>
				</ALGORITHMS>
				<FEATURES>
					<FEATURE>F1</FEATURE>
					<FEATURE>F2</FEATURE>
				</FEATURES>
			</CONFIGURATION>
			<CONFIGURATION SID="CONF2" DESC="">
				<SAMPLING_METHOD>SM1</SAMPLING_METHOD>
				<DATASET>DS1</DATASET>
				<HARDWARE>HC1</HARDWARE>
				<ALGORITHMS>
					<ALGORITHM>A1</ALGORITHM>
					<ALGORITHM>A2</ALGORITHM>
				</ALGORITHMS>
				<FEATURES>
					<FEATURE>F1</FEATURE>
					<FEATURE>F2</FEATURE>
					<FEATURE>F3</FEATURE>
					<FEATURE>F4</FEATURE>
				</FEATURES>
			</CONFIGURATION>
		</CONFIGURATIONS>
	</MEXCORE>
	<MEXALGO>
		<ALGORITHMS>
			<A SID="A1" MEX_ID="CSVM">
				<P ID="C" VALUE="10^3"></P>
				<P ID="alpha" VALUE="0.01"></P>
			</A>
			<A SID="A2" MEX_ID="NB"></A>
			<A SID="A3" MEX_ID="CSVM">
				<P ID="C" VALUE="10^2"></P>
				<P ID="alpha" VALUE="0.01"></P>
			</A>
		</ALGORITHMS>
	</MEXALGO>
	<MEXPERF>
		<MEASURES>
			<M SID="M1">ACCURACY</M>
			<M SID="M2">TPR</M>
		</MEASURES>
	</MEXPERF>
	<OTHERS>
		<EXECUTION_TYPE_TRAIN>OVERALL</EXECUTION_TYPE_TRAIN>
		<EXECUTION_TYPE_TEST>SINGLE</EXECUTION_TYPE_TEST>
		<LOG_TRAIN>
	</OTHERS>	
</MyMEX>